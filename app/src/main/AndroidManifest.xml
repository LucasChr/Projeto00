        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Principal.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Listas.TuristicoListActivity" />
        <activity android:name=".Listas.EmpresarialListActivity" />
        <activity android:name=".PEmpresarial.EmpresarialCadActivity" />
        <activity android:name=".PTuristico.TuristicoCadActivity" />
        <activity android:name=".Mapa.MapaActivity"></activity>
    </application>

</manifest>